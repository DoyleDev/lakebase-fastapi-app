# Databricks Configuration
DATABRICKS_DATABASE_INSTANCE=<INSTANCE_NAME>
DATABRICKS_DATABASE_NAME=<DATABASE_NAME>
DATABRICKS_DATABASE_PORT=5432

# Table Configuration
DEFAULT_POSTGRES_SCHEMA=<SCHEMA_NAME>
DEFAULT_POSTGRES_TABLE=<TABLE_NAME>

# Databricks App Authentication (set these for production deployment)
DATABRICKS_HOST=<DATABRICKS_HOST> 
DATABRICKS_TOKEN=<PAT_TOKEN>
DATABRICKS_USER_NAME=<YOUR_USER_NAME>

# Database Connection Pool Settings
DB_POOL_SIZE=5
DB_MAX_OVERFLOW=10
DB_COMMAND_TIMEOUT=30
DB_POOL_TIMEOUT=10